<!DOCTYPE html>
<html>
<head>
  <title>API Data Rendering</title>
</head>
<body>
  <h1>API Data Rendering</h1>

  <% data.employees.forEach(function(employee) { %>
    <div class="employee">
      <h2><%= employee.name %></h2>
      <p><strong>Designation:</strong> <%= employee.designation || 'Not available' %></p>
    </div>
  <% }) %>
</body>
</html>
